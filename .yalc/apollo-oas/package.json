{
  "name": "apollo-oas",
  "version": "1.0.11",
  "description": "Generates an Apollo Connector from an OpenAPI 3.x specification (YAML or JSON)",
  "author": "Fernando @ Apollo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fernando-apollo/oas-to-connector.git"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "module-sync": "./dist/index.js",
      "require": null,
      "default": "./dist/index.js"
    },
    "./json": {
      "types": "./dist/json/index.d.ts",
      "module-sync": "./dist/json/index.js",
      "require": null,
      "default": "./dist/json/index.js"
    },
    "./oas": {
      "types": "./dist/oas/index.d.ts",
      "module-sync": "./dist/oas/index.js",
      "require": null,
      "default": "./dist/oas/index.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "prebuild": "rm -r dist",
    "build": "tsc",
    "test": "node --import tsx/esm --test 'tests/oas.test.ts'",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint",
    "prepublishOnly": "npm run build && npm test && publint",
    "preversion": "npm run lint && npm test",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@inquirer/checkbox": "^4.1.1",
    "@inquirer/core": "^10.1.7",
    "@inquirer/prompts": "^7.3.1",
    "commander": "^13.1.0",
    "lodash": "^4.17.21",
    "oas": "^25.3.0",
    "oas-normalize": "^12.1.0",
    "yoctocolors-cjs": "^2.1.2"
  },
  "yalcSig": "4c28b3a8ba948b5b0b6b0a73cc7bd3ac"
}
